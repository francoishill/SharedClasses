						<Setter Property="Template">
							<Setter.Value>
								<ControlTemplate TargetType="{x:Type Thumb}">
									<Border Background="#FF777777"
													CornerRadius="2" />
								</ControlTemplate>
							</Setter.Value>
						</Setter>
					</Style>
