									<ControlTemplate.Triggers>
										<Trigger Property="IsEnabled"
														 Value="false">
											<Setter TargetName="Bg"
															Property="Visibility"
															Value="Hidden" />
										</Trigger>
									</ControlTemplate.Triggers>
								</ControlTemplate>
							</Setter.Value>
